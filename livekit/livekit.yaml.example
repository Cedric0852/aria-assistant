# LiveKit Server Configuration
# Copy this file to livekit.yaml and update the API keys

port: 7880

rtc:
  port_range_start: 50000
  port_range_end: 60000
  udp_port: 7882
  tcp_port: 7881
  use_external_ip: true

redis:
  address: redis:6379

room:
  auto_create: true
  empty_timeout: 300
  max_participants: 100

logging:
  level: info
  json: false

# API keys - must match LIVEKIT_API_KEY and LIVEKIT_API_SECRET in .env
# Generate a secure secret for production use
keys:
  devkey: your-livekit-api-secret-here
